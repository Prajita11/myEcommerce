{
  "name": "amit_dhoju_app",
  "description": "Web Application with React, Apollo Client, GraphQL, Tailwind CSS, and Vite",
  "author": {
    "name": "Amit Dhoju",
    "email": "amitdhoju@gmail.com",
    "url": "https://github.com/Amit091"
  },
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint --quiet 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "lint:fix": "eslint --quiet --fix 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "precommit": "npm run lint && npm run format",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:debug": "graphql-codegen --config codegen.ts --debug"
  },
  "dependencies": {
    "@apollo/client": "^3.10.8",
    "antd": "^5.26.7",
    "apollo-upload-client": "^18.0.1",
    "axios": "^1.7.2",
    "dotenv": "^16.4.7",
    "graphql": "^16.9.0",
    "i18next": "^25.0.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "i18next-resources-to-backend": "^1.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.1",
    "react-router-dom": "^6.25.1",
    "react-spinners": "^0.14.1",
    "vite-plugin-pwa": "^0.20.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.3.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ]
  }
}
